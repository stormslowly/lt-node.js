{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsc",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "lt-codes-nodejs",
  "author": "pshu",
  "module": "dist/lt-codes-nodejs.esm.js",
  "size-limit": [
    {
      "path": "dist/lt-codes-nodejs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lt-codes-nodejs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/express": "^4.17.11",
    "husky": "^5.0.9",
    "size-limit": "^4.9.2",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "qrcode-camera-decode": "^1.0.1",
    "qrcode-terminal": "^0.12.0"
  }
}
